{{#if productosCargados}}
<div>
    <h1>Cargar Producto</h1>
    <table>
        <tr>
            <th>Producto</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        
                     {{#each productos}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.precio}}</td>
            <td><img src={{this.imagen}} alt=""></td>
                
        </tr>
             {{/each}}
    </table>
</div>
{{else}}
<h1>No hay productos</h1>
    <br>
    <a href="/" type="button" class="btn btn-primary">Cargar un nuevo producto</a>
